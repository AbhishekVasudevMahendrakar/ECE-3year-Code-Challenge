i = "Micheal"
j = "e3$WT89x"
for i,j in  range(0,3):
   if i == "Micheal" or j == "e3$WT89x":
      print ("valid")
   
   elif i != "Micheal" or j != "e3$WT89x":
      print ("invalid")
      break
   else:
      print ("account has been blocked")

